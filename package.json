{
  "name": "parcel-web-app",
  "version": "1.0.0",
  "displayName": "Parcel Web App",
  "description": "Standard template for developing web interfaces with Parcel.",
  "main": "./dist/index.js",
  "bin": {
    "@ducho/debug": "./dist/index.js"
  },
  "scripts": {
    "dev": "ncc build ./src/index.ts -w -o dist/",
    "build": "ncc build ./src/index.ts -o ./dist/ --minify --no-cache --no-source-map-register"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@vercel/ncc": "^0.38.1",
    "commander": "^12.1.0",
    "typescript": "^5.4.5"
  }
}
